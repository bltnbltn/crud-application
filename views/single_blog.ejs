<!-- include header -->
<%- include('layout/header') %>
  <!-- /include header -->


  <main id="site-main">
    <div class="container">

      <div class="blog-box">
        <div class="blog-img">
          <img src="<%= blog.imageUrl %>" alt="blog" />
        </div>

        <div class="blog-text">
          <span>
            <%= blog.date %> / <%= blog.category %>
          </span>
          <h1 class="blog-title">
            <%= blog.title %>
          </h1>
          <!-- Add more blog details here -->
        </div>
      </div>

  </main>

  <!-- include footer -->
  <%- include('layout/footer') %>
    <!-- /include footer -->